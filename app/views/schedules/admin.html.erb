<p id="notice"><%= notice %></p>
<h1>Admin Panel</h1>

<table class="table table-hover">
  <thead>
  <tr>
    <th>First name</th>
    <th>Last name</th>
    <th>Age</th>
    <th>Dob</th>
    <th>City</th>
    <th>State</th>
    <th>Zipcode</th>
    <th>Urgency</th>
    <th>Time</th>
    <th>Note</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @schedules.each do |schedule| %>
    <tr>
      <td><%= schedule.first_name %></td>
      <td><%= schedule.last_name %></td>
      <td><%= schedule.age %></td>
      <td><%= schedule.dob %></td>
      <td><%= schedule.city %></td>
      <td><%= schedule.state %></td>
      <td><%= schedule.zipcode %></td>
      <td><%= schedule.urgency %></td>
      <td><%= schedule.time %></td>
      <td><%= schedule.note %></td>
      <td><%= link_to 'Edit', edit_schedule_path(schedule) %></td>
      <td><%= link_to 'Destroy', schedule, method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
  </tbody>
</table>

<br>
